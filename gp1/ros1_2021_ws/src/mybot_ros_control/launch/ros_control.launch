<?xml version="1.0"?>
<launch>

		<!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find mybot_ros_control)/config/controllers.yaml" command="load" />
    
		<!-- Load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" 
			respawn="false" output="screen" args="joints_update mobile_base_controller" />

</launch>
